@model IEnumerable<Crud_Asp.net.Models.Empregado>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="AddOrEdit">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NameCompleto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmpCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Posicao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LocalizacaoTrabalho)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NameCompleto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmpCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Posicao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LocalizacaoTrabalho)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EmpregadoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EmpregadoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EmpregadoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
